a = int(input("请输入学生人数："))
num_list = []
name_list = []
d = {}
for i in range(a):
    num = input("学号为：")
    name = input("姓名为：")
    print("\n")
    num_list.append(num)
    name_list.append(name)

d = dict(zip(num_list, name_list))
new_d = dict(sorted(d.items(), key=lambda x: x[0]))
print(new_d)
